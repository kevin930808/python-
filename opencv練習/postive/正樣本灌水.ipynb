   "source": [
    "import os\n",
    "import cv2\n",
    "path='./'\n",
    "imgs=os.listdir(path)\n",
    "n=0\n",
    "for img in imgs:\n",
    "    if '.jpg' in img:\n",
    "        i = cv2.imread(img)\n",
    "        cv2.imwrite('./new/'+str(n)+'.jpg',i)\n",
    "        n+=1\n",
    "        j=cv2.flip(i,1)\n",
    "        cv2.imwrite('./new/'+str(n)+'.jpg',j)\n",
    "        n+=1\n"
   ]
